[server]
protocol = http
http_addr = 0.0.0.0
http_port = {{ grafana_port }}
domain = {{ grafana_domain }}
root_url = http://{{ grafana_domain }}:{{ grafana_port }}/

[security]
admin_user = {{ grafana_admin_user }}
admin_password = {{ grafana_admin_password }}

[users]
allow_sign_up = false

[auth.anonymous]
enabled = false

[database]
type = sqlite3
path = grafana.db

[session]
provider = file

[analytics]
reporting_enabled = false
check_for_updates = false

[log]
mode = console file
level = info
